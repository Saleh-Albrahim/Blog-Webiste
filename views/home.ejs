<%- include("partials/header") %>
<h1>Home</h1>
<p><%=homeStartingContent%></p>
<% posts.forEach(element => { %>
<h1><%=element.title%></h1>
<% if(element.content.length > 100) { %>
<p><%=element.content.substring(0,100)+" ..."%>
    <a href="<%=`/posts/${element._id}`%>">Read More</a>
</p>
<% }else{  %>
<p><%=element.content%></p>
<% } %>
<%});%>
<%- include("partials/footer") %>